<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" name="es.uji.control.sip.preferences">
   <implementation class="es.uji.control.sip.preferences.internal.PreferenceManagerSupplier"/>
   <property name="dependency.injection.annotation" type="String" value="org.fipro.e4.service.preferences.PrefMgr"/>
   <service>
      <provide interface="org.eclipse.e4.core.di.suppliers.ExtendedObjectSupplier"/>
   </service>
   <reference bind="addPreferenceStoreFactory" cardinality="1..1" interface="es.uji.control.sip.preferences.IPreferenceStoreFactoryService" name="IPreferenceStoreFactoryService" policy="static" unbind="removePreferenceStoreFactory"/>
   <reference bind="addPreferenceNode" cardinality="0..n" interface="es.uji.control.sip.preferences.PreferenceNodeContribution" name="PreferenceNodeContribution" policy="dynamic" unbind="removePreferenceNode"/>
</scr:component>
